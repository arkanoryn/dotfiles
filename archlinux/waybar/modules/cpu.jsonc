{
  "cpu": {
    "interval": 10,
    "format": "󰍛 {usage}%",
    "rotate": 90,
    "format-alt": "{icon0}{icon1}{icon2}{icon3}",
    "format-icons": ["▁", "▂", "▃", "▄", "▅", "▆", "▇", "█"]
  },

  "custom/cpuinfo": {
    "exec": " cpuinfo.sh",
    "return-type": "json",
    "format": "{}",
    "rotate": 90,
    "interval": 5, // once every 5 seconds
    "tooltip": true,
    "max-length": 1000
  }
}
